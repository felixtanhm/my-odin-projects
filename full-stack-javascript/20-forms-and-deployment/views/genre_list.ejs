<!DOCTYPE html>
<html>
  <%- include('./partials/header') %>
  <body>
    <%- include('./partials/nav.ejs') %>
    <main>
      <div class="content">
        <h1 class=""><%= title %></h1>
        <% if (genre_list.length) { %>
        <ul>
          <% genre_list.forEach(genre => { %>
          <li>
            <a href="<%= genre.url %>"><%= genre.name %></a>
          </li>
          <% }) %>
        </ul>
        <%} else { %>
        <p>There are no genres.</p>
        <% } %>
      </div>
      <%- include('./partials/footer') %>
    </main>
  </body>
</html>
